<head> 
  <meta charset="utf-8"> 
 
  <!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame
       Remove this if you use the .htaccess --> 
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> 
 
  <title>Lacanau{% if title is not empty %} - {{ title }}{% endif %}</title> 
  <meta name="description" content=""> 
  <meta name="author" content="Lukas Botsch"> 
 
  <!-- Mobile viewport optimized: j.mp/bplateviewport --> 
  <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
 
  <!-- Place favicon.ico & apple-touch-icon.png in the root of your domain and delete these references --> 
  <link rel="shortcut icon" href="/favicon.ico"> 
  <link rel="apple-touch-icon" href="/apple-touch-icon.png"> 
 
 
  <!-- CSS: implied media="all" --> 
  <link rel="stylesheet" href="{{ asset('bundles/lubocontentmanager/css/style.css') }}"> 
  <link rel="stylesheet" href="{{ asset('bundles/lubocontentmanager/css/custom-theme/jquery-ui-1.8.13.custom.css') }}">
  
  <!-- Uncomment if you are specifically targeting less enabled mobile browsers
  <link rel="stylesheet" media="handheld" href="css/handheld.css?v=2">  --> 
 
  <!-- All JavaScript at the bottom, except for Modernizr which enables HTML5 elements & feature detects --> 
  <script src="{{ asset('bundles/lubocontentmanager/js/libs/modernizr-1.7.min.js') }}"></script> 
 
</head> 
 
<body>
  <div id="container"> 
    <header class="clearfix"> 
      <div id="lang-switcher"> 
        <ul> 
          <li><a href="{{ path('homepage', {'_locale': 'de'})}}"><img src="{{ asset('bundles/lubocontentmanager/img/flag_de.svg') }}" /></a></li> 
          <li><a href="{{ path('homepage', {'_locale': 'fr'})}}"><img src="{{ asset('bundles/lubocontentmanager/img/flag_fr.svg') }}" /></a></li> 
          <li><a href="{{ path('homepage', {'_locale': 'en'})}}"><img src="{{ asset('bundles/lubocontentmanager/img/flag_en.svg') }}" /></a></li> 
        </ul> 
      </div>
      {% block title_area %}
      <div id="title-area">
        {{ page_area('title', {'global': true})|raw }}
      </div>
      {% endblock %} 
    </header>
    <br style="clear:both;">
    <div id="main" role="main"> 
      <div class="bubble"> 
        <section>
            {% block content_area %}
                {{ page_area('content')|raw }}
            {% endblock %}
        </section> 
      </div> 
    </div> 
    <footer>
      {% block footer_area %}
          {{ page_area('main_menu', {'allowed_slots': ['menu'], 'global': true})|raw }}
      {% endblock %} 
    </footer> 
  </div> <!--! end of #container --> 
 
 
  <!-- JavaScript at the bottom for fast page loading --> 
 
  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if necessary --> 
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script> 
  <script>window.jQuery || document.write("<script src='{{ asset('bundles/lubocontentmanager/js/libs/jquery-1.6.1.min.js') }}'>\x3C/script>")</script> 
  <!-- <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/jquery-ui.min.js"></script> -->
  <script src="{{ asset('bundles/lubocontentmanager/js/libs/jquery-ui-1.8.13.custom.min.js') }}"></script>
  
  <!-- scripts concatenated and minified via ant build script--> 
  <script src="{{ asset('bundles/lubocontentmanager/js/plugins.js') }}"></script> 
  <script src="{{ asset('bundles/lubocontentmanager/js/script.js') }}"></script> 
  <!-- end scripts--> 
 
 
  <!--[if lt IE 7 ]>
    <script src="{{ asset('bundles/lubocontentmanager/js/libs/dd_belatedpng.js') }}"></script>
    <script>DD_belatedPNG.fix("img, .png_bg"); // Fix any <img> or .png_bg bg-images. Also, please read goo.gl/mZiyb </script>
  <![endif]--> 
 
 
